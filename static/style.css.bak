/* css reset  */
body{
    margin: 0;
    padding: 0;
}
/* Navigation Bar  */
#navbar{
    display: flex;
    position: relative;
    height: 75px;
}
#navbar::before{
    content: "";
    background-color: rgb(100, 7, 7);
    position: absolute;
    height: 100%;
    width: 100%;
    /* z index is required to bring text to front */
    z-index: -1;
    opacity: 0.;
    top: 0px;
    left: 0px;
}
   
/* Logo text */
#logo{
    margin: 20px 60px;
}
#logo span{
    color: black;
    font-size: 1.8rem;

}
#navbar ul{
    font-family: 'Balsamiq Sans', cursive;
    display: flex;
}
#navbar ul li{
    list-style: none;
    margin: 5px;
    font-size: 1.3rem;
    
}
#navbar ul li a{
    color: black;
    /* display:block  required for padding   deafault is inline */
    display: block;
    padding: 5px 41px;
    text-decoration: none;
    border-radius: 25px;
}
#navbar ul li a:hover{  
    
    cursor: pointer; 
    text-decoration: underline;
    color: black;
    background-color: rgb(214, 205, 176);
      
}

/* HOME Section */
#home{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    height: 477px;
}
#home::before{
    content: "";
    position: absolute;
    background:url("../static/bg3.jpg") no-repeat center center/cover ;
    z-index: -1;
    height: 100%;
    width: 100%;
    opacity: 0;
    top: 0px;
    left: 0px;
}


#home .h-primary{
    font-family: 'Balsamiq Sans', cursive;
margin: 0px;
    font-size: 3rem;
}
#home p{
    font-family: 'Audiowide', cursive;
margin: 0px;
    font-size: 2.5rem;
}

/* edit page Section */
#edit{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    height: 477px;
}
#edit::before{
    content: "";
    position: absolute;
    background:url("../static/edit.jpg") no-repeat center center/cover ;
    z-index: -1;
    height: 100%;
    width: 100%;
    opacity: 0.8;
    top: 0px;
    left: 0px;
}


#edit .h-primary{
    font-family: 'Balsamiq Sans', cursive;
margin: 0px;
    font-size: 3rem;
}
#edit p{
    font-family: 'Audiowide', cursive;
margin: 0px;
    font-size: 2.5rem;
}


